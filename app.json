{
  "expo": {
    "name": "F-ERP",
    "slug": "F-ERP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/F-ERP_Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/F-ERP_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.hieumai1507.FERP"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/F-ERP_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "package": "com.hieumai1507.FERP"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/F-ERP_Logo.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "684e7ebf-6aa4-4997-a976-7a399781def7"
      }
    }
  }
}
