{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(redux)/authSlice.js",
    "app/(redux)/store.js",
    "app/auth/LoginScreen.js",
    "components/AppGradient.tsx",
    "app/_layout.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/CalendarScreen.jsx",
    "app/(tabs)/HomeScreen.jsx",
    "app/(tabs)/PersonalScreen.jsx",
    "app/(tabs)/ProfileScreen.jsx",
    "app/request/createRequest.jsx",
    "app/request/request.jsx",
    "app/index.jsx",
    "components/APICheckInEveryDay.js"
  ]
}
